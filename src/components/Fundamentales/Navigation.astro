---
import NavBar from "../Fundamentales/NavBar.jsx";
import ThemeIcon from "../ChangeUI/ThemeIcon.astro";
const url = Astro.url;
const pathName = Astro.url.pathname;
const { cookies } = Astro;

const accessToken = cookies.get("sb-access-token");
const refreshToken = cookies.get("sb-refresh-token");
---

<header class="w-full h-max">
  <NavBar url={url} pathName={pathName} accessToken={accessToken} refreshToken={refreshToken} client:load>
    <ThemeIcon/>
  </NavBar>
</header>
