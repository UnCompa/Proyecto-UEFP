{
	"version": 3,
	"routes": [
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"handle": "filesystem"
		},
		{
			"src": "^\\/_image$",
			"dest": "_render"
		},
		{
			"src": "^\\/404\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/api\\/auth\\/register\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/api\\/auth\\/signin\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/api\\/auth\\/signout\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/dashboard\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/en\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/en\\/about\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/en\\/academics\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/en\\/contacts\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/en\\/dashboard\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/en\\/news\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/en\\/register\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/en\\/signin\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/$",
			"dest": "_render"
		},
		{
			"src": "^\\/es\\/about\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/es\\/academics\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/es\\/contacts\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/es\\/dashboard\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/es\\/news\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/es\\/register\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/es\\/signin\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/$",
			"dest": "_render"
		},
		{
			"src": "^\\/register\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/signin\\/?$",
			"dest": "_render"
		},
		{
			"src": "/.*",
			"dest": "_render",
			"status": 404
		}
	]
}